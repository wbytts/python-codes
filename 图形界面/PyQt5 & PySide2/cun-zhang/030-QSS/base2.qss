QPushButton:hover{
    background-color: red;
}

QPushButton:pressed{
    background-color: green;
}

QCheckBox::indicator{
    width:20px;
    height:20px;
}

QCheckBox::indicator:checked:hover{
    image:url(y.png);
    border:2px solid red;
}

QCheckBox::indicator:indeterminate{
    image:url(../2.png);
}

QCheckBox::indicator:unchecked{
    image:url(n.png);
}
